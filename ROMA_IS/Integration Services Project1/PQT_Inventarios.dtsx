      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{EE8785FD-6D43-4F47-BAF3-145D7203E39B}"
          SQLTask:SqlStatementSource="--Cajas=N&#xA;--Pzas=Y&#xA;--Declare @flagUM char(1)&#xA;&#xA;If OBJECT_ID('tempdb..#tmpreporte', N'U') Is Not Null Drop Table #tmpreporte&#xA;If OBJECT_ID('tempdb..#tmpproalm', N'U') Is Not Null Drop Table #tmpproalm&#xA;If OBJECT_ID('tempdb..#tmpfecmax', N'U') Is Not Null Drop Table #tmpfecmax&#xA;If OBJECT_ID('tempdb..#tmppreciocompra', N'U') Is Not Null Drop Table #tmppreciocompra&#xA;If OBJECT_ID('tempdb..#tmppa', N'U') Is Not Null Drop Table #tmppa&#xA;If OBJECT_ID('tempdb..#tmpalm', N'U') Is Not Null Drop Table #tmpalm &#xA;&#xA;Declare &#xA;&#x9;@as_proveedor Varchar= '11',&#xA;&#x9;@ls_flagH Char(1)&#xA;&#x9;&#xA;Set @ls_flagH = Case When @as_proveedor='PM00000027' Then 'S' Else 'N' End&#xA;    &#xA;select itemcode, max(fecha) as fecha &#xA;into #tmpfecmax&#xA;from Reportes.ultpreciocompra &#xA;group by itemcode&#xA;&#xA;-- Precio compra--&#x9;&#x9;&#xA;select a.itemcode as codpro,a.costo as price &#xA;into #tmppreciocompra &#xA;from Reportes.ultpreciocompra a,#tmpfecmax b &#xA;where  a.itemcode=b.itemcode and a.fecha=b.fecha&#xA;    &#xA;&#x9;&#x9;&#x9;&#xA;SELECT b.cardcode, a.itemcode as codpro,a.whscode as codalm,a.onhand as stock &#xA;into #tmpproalm&#xA;FROM oitw a, oitm b where a.itemcode=b.itemcode -- and b.cardcode=@as_proveedor &#xA;and substring(a.whscode,5,2)='PR'&#xA;and b.frozenfor='N'&#xA;and a.whscode in ('11ICPR1','11CHPR1','11NZPR1','05AYPR1')&#xA;and (&#xA;&#x9;&#x9;(b.CardCode='PM00000027' and a.whscode in ('11ICPR1','11CHPR1','11NZPR1','05AYPR1'))&#x9;&#xA;&#x9;&#x9;Or &#xA;&#x9;&#x9;(b.CardCode!='PM00000027' and a.whscode in ('11ICPR1','11CHPR1','11NZPR1','05AYPR1'))&#xA;&#x9;)&#xA;-- Select * From #tmpproalm&#xA;&#xA;select &#xA;--c.grupo,c.pareto/cast(100 as numeric(15,2)) as pareto,&#xA;a.codalm, a.cardcode codprv,&#xA;a.codpro as codpro,b.itemname as nompro,b.buyunitmsr as uni_compra,&#xA;b.numinbuy as numpza_ucompra,&#xA;e.price*(1) as Precio_compra_xCajas,&#xA;e.price*(b.numinbuy) as Precio_compra_xPzas,&#xA;round(a.stock/cast((1) as numeric(15,2)),0) as stock_xPzas,&#xA;round(a.stock/cast((b.numinbuy) as numeric(15,2)),0) as stock_xCajas,&#xA;isnull(stock*(case b.U_BPP_TIPUNMED when '01' then isnull(ume.valor1,0) when '07' then 1/(ume.valor1) when '08' then isnull(ume.valor1,0) when '12' then 1/(ume.valor1) else 0 end ),0) stock_umes&#xA;--round(isnull(isnull(d.vtaprommes,0),0)/cast((case @flagUM when 'Y' then 1 else  b.numinbuy end ) as numeric(15,2)),2) as vta_um,&#xA;--round(isnull(d.vtapromdia/cast((case @flagUM when 'Y' then 1 else  b.numinbuy end ) as numeric(15,2)),0),2) as vta_ultdias,&#xA;--cast(0 as numeric(15,2)) as dias_stock,&#xA;--cast(0 as numeric(15,2)) as valorizado&#xA;--cast(0 as numeric(15,2)) as excedente_valorizado,&#xA;--cast(0 as numeric(15,2)) as stock_excedente,&#xA;--cast(0 as numeric(15,2)) as stock_tt,&#xA;--cast(0 as numeric(15,2)) as vta_um_tt,&#xA;--cast(0 as numeric(15,2)) as vta_ultdias_tt,&#xA;--cast(0 as numeric(15,2)) as dias_stock_tt,&#xA;--cast(0 as numeric(15,2)) as excedente_valorizado_tt,&#xA;--cast(0 as numeric(15,2)) as stock_umes_tt&#xA;Into #tmpreporte&#xA;-- Select *&#xA;From #tmpproalm a&#xA;inner join oitm b on (a.codpro=b.itemcode And a.CardCode = b.CardCode)&#xA;--left outer join Reportes.paretoprv c on (a.codpro=c.codpro)&#xA;--left outer join reportes.vtapromdm  d on (a.codalm=d.codalm and a.codpro=d.codpro)&#xA;left outer join #tmppreciocompra e on (a.codpro=e.codpro)&#xA;left outer join (select * from dw.reportes.item_unidadvolumenpeso where valor1 &gt;0) ume on b.ItemCode=ume.itemcode And ume.CodPrv = a.CardCode&#xA;--where cast(d.fecha as date)=cast(getdate() as date)&#xA;--and isnull(c.pareto/cast(100 as numeric(15,2)),0)&gt;0&#xA;--and a.codpro in ('INT130025','INT700066')&#xA;-- insertar con cero en los almcenes que no ha tenido venta&#xA;-- productos por almacen&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;select distinct codpro,codalm into #tmppa from #tmpreporte&#xA;select distinct codalm into #tmpalm from #tmpreporte&#xA;&#xA;&#xA;&#xA;-- Select * From #tmpreporte&#xA;&#xA;insert into #tmpreporte&#xA;select a.codalm, a.CardCode, a.codpro as codpro,b.itemname as nompro,b.buyunitmsr as uni_compra,&#xA;b.numinbuy as numpza_ucompra,e.price*b.numinbuy as Precio_compra, 0,&#xA;round(a.stock/cast((1)  as numeric(15,2)),0) as stock_xCajas,&#xA;round(a.stock/cast((b.numinbuy)  as numeric(15,2)),0) as stock_Pzas,&#xA;isnull(stock*(case b.U_BPP_TIPUNMED when '01' then isnull(ume.valor1,0) when '07' then 1/(ume.valor1) when '08' then isnull(ume.valor1,0) when '12' then 1/(ume.valor1) else 0 end ),0) stock_umes&#xA;----0 as vta_um,&#xA;----0 as vta_ultdias,&#xA;--cast(0 as numeric(15,2)) as dias_stock,&#xA;--cast(0 as numeric(15,2)) as valorizado,&#xA;--cast(0 as numeric(15,2)) as excedente_valorizado,&#xA;--cast(0 as numeric(15,2)) as stock_excedente,&#xA;--cast(0 as numeric(15,2)) as stock_tt,&#xA;--cast(0 as numeric(15,2)) as vta_um_tt,&#xA;--cast(0 as numeric(15,2)) as vta_ultdias_tt,&#xA;--cast(0 as numeric(15,2)) as dias_stock_tt,&#xA;--cast(0 as numeric(15,2)) as excedente_valorizado_tt,0&#xA;from #tmpproalm a inner join Reportes.paretoprv c on (a.codpro=c.codpro)&#xA;inner join oitm b on (a.codpro=b.itemcode)&#xA;left outer join #tmppreciocompra e on (a.codpro=e.codpro)&#xA;left outer join (select * from dw.reportes.item_unidadvolumenpeso where valor1 &gt;0) ume on b.ItemCode=ume.itemcode  &#xA;where  isnull(c.pareto/cast(100 as numeric(15,2)),0)&gt;0 &#xA;and not exists (select 1 from #tmppa pp where pp.codpro=a.codpro and pp.codalm=a.codalm)&#xA;--and a.codpro in ('INT130025','INT700066')&#xA;&#xA;Truncate Table temp.Tmp_Stock &#xA;Insert Into temp.Tmp_Stock &#xA;select * from #tmpreporte Where stock_xCajas&gt;0&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
