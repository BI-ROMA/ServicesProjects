    <DTS:Executable
      DTS:refId="Package\DimProductosFull Dim_Categorias_xProveedor"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Tarea Ejecutar SQL"
      DTS:DTSID="{3F766EA1-A927-450B-9335-6CB4B3A94F95}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="DimProductosFull Dim_Categorias_xProveedor"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2019; © 2019 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{6603F4A0-7C8D-41DE-A9FC-9048126A5E5A}"
          SQLTask:SqlStatementSource="--Drop Table If Exists #ProdsInv&#xA;--Drop Table If Exists #ItemCodeVta&#xA;&#xA;Select Distinct SWW U_BKS_GRUPO, a.CodPro&#xA;Into #ProdsInv&#xA;From ROMA_REPOSITORIO.temp.Inventarios a&#xA;Inner Join SrvSAP.Roma_Productiva.dbo.OITM aa On a.CodPro Collate SQL_Latin1_General_CP1_CI_AS = aa.ItemCode&#xA;Where Not Exists (Select * From ventas.DimProductosFull b Where a.CodPro = b.CodPro)&#xA;And a.Stock &gt; 0&#xA;&#xA;Select Distinct SWW U_BKS_GRUPO, a.itemcode &#xA;Into #ItemCodeVta&#xA;From SrvSAP.DW.Reportes.item_unidadvolumenpeso a&#xA;Inner Join SrvSAP.Roma_Productiva.dbo.OITM aa On a.ItemCode = aa.ItemCode And a.CodPrv = aa.CardCode&#xA;Where Convert(char(6), feregistro, 112) &gt;= Convert(char(6), getdate()-60, 112) &#xA;And Not Exists(Select * From Ventas.DimProductosFull b Where a.codprv = b.CodPrv  Collate SQL_Latin1_General_CP850_CI_AS And a.itemcode = b.codpro  Collate SQL_Latin1_General_CP850_CI_AS)&#xA;&#xA;Insert Into #ProdsInv&#xA;Select * From #ItemCodeVta a&#xA;Where Not Exists (Select * From #ProdsInv b Where a.ItemCode Collate SQL_Latin1_General_CP1_CI_AS = b.CodPro)&#xA;&#xA;Insert Into Ventas.DimProveedores (CodPrv, NomPrv, NomPrvAbr, Origen)&#xA;Select Distinct c.CardCode, CardName, CardFName, 'SSIS' From #ProdsInv a&#xA;Inner Join SrvSAP.roma_productiva.dbo.OITM b On a.CodPro = b.itemcode Collate SQL_Latin1_General_CP1_CI_AS&#xA;Inner Join SrvSAP.roma_productiva.dbo.OCRD c On b.cardcode = c.cardcode&#xA;Where Not Exists (Select * From Ventas.DimProveedores x Where x.CodPrv = c.CardCode Collate SQL_Latin1_General_CP1_CI_AS)&#xA;&#xA;Insert Into ventas.Dim_Categorias_xProveedor(CodCat, NomCat, NomCat_Abrv, IdPrv)&#xA;Select '000' CodCat, 'NO DEFINIDO' NomCat, 'NO DEFINIDO' NomCat_Abrv, IdPrv&#xA;From Ventas.DimProveedores a&#xA;Where DateAdd(MONTH, -2, Getdate()) &lt; FeRegistro &#xA;And Not Exists (Select * From Ventas.Dim_Categorias_xProveedor b Where a.IdPrv = b.IdPrv)&#xA;&#xA;-- Select * From Ventas.Dim_Categorias_xProveedor Where CodCat = '000'&#xA;&#xA;--Select CardCode, CardFName, CardName From OCRD Where CardCode = 'PM00000099'&#xA;----Select * From #ProdsInv&#xA;--Select * From SrvSAP.dw.Reportes.item_unidadvolumenpeso Where itemcode='BLE030133'&#xA;--Select * From ventas.DimProductosFull Where CodPro = 'BLE030133'&#xA;---- &#x9;   &#xA;--Select * From Ventas.DimProveedores Where codprv= 'PM00000099'&#xA;&#xA;&#xA;Insert Into ventas.DimProductosFull(IdPrv, CodPro, NomPro, CodPrv, CodFam, NomFam, U_BKS_CATEGORIA, U_BKS_CATEGORIA_Nom, CodCat, NomCat, &#xA;CodFamNestle, NomFamNestle, catJes, U_BKS_GRUPO, CoUnidadMedidaCompra, QtdPiezas)&#xA;Select IdPrv, a.ItemCode CodPro, ItemName NomPro, a.CodPrv, IsNull(CodFam, '000'), IsNull(z.Name, 'NO DEFINIDO') NomFam, &#xA;Case When IsNull(U_BKS_CATEGORIA, '') = '' Then '000' Else U_BKS_CATEGORIA End U_BKS_CATEGORIA, &#xA;Case When IsNull(U_BKS_CATEGORIA, '') = '' Then 'NO DEFINIDO' Else y.name End U_BKS_CATEGORIA_Nom, &#xA;CodCat, Null NomCat, CodCat CodFamNestle, '*' NomFamNestle, catJes, SWW U_BKS_GRUPO, x.buyunitmsr, x.numinbuy&#xA;From SrvSAP.dw.Reportes.item_unidadvolumenpeso a&#xA;Inner Join SrvSAP.Roma_Productiva.dbo.OITM x On a.ItemCode = x.ItemCode&#xA;Left Join SrvSAP.Roma_Productiva.dbo.[@BKS_CATEGORIA] y On x.U_BKS_CATEGORIA = y.Code&#xA;Left Join SrvSAP.Roma_Productiva.dbo.[@BKS_FAMILIAS] z On x.U_BKS_FAMILIA = z.Code&#xA;Inner Join Ventas.DimProveedores prv On prv.CodPrv = a.codprv Collate SQL_Latin1_General_CP1_CI_AS&#xA;Where (/*a.FeRegistro &gt;= Getdate()-30 Or */a.ItemCode Collate SQL_Latin1_General_CP1_CI_AS In (Select CodPro From #ProdsInv))&#xA;And Not Exists (Select * From ventas.DimProductosFull b &#xA;&#x9;&#x9;Where a.codprv Collate SQL_Latin1_General_CP1_CI_AS = b.codprv &#xA;&#x9;&#x9;and a.itemcode Collate SQL_Latin1_General_CP1_CI_AS = b.codpro)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  &#xA;&#xA;&#xA;Update a Set CodCat = IsNull(b.CodCat, '000'), CodFam = IsNull(b.Codfam, 'FAM000'), CatJes=IsNull(b.catjes, 'NO DEFINIDO')&#x9;&#xA;From Ventas.DimProductosFull a&#xA;Inner Join SrvSAP.dw.Reportes.item_unidadvolumenpeso b &#xA;On a.CodPrv = b.CodPrv Collate SQL_Latin1_General_CP1_CI_AS&#xA;And a.CodPro = b.ItemCode Collate SQL_Latin1_General_CP1_CI_AS&#xA;Where IsNull(b.CodCat, '000') &lt;&gt; '000'&#xA;And (&#xA;&#x9;IsNull(b.CodCat, '000') Collate SQL_Latin1_General_CP1_CI_AS&lt;&gt; IsNull(a.CodCat, '000')&#xA;&#x9;Or &#xA;&#x9;IsNull(b.CodFam, 'FAM000') Collate SQL_Latin1_General_CP1_CI_AS&lt;&gt; IsNull(a.CodFam, 'FAM000')&#xA;&#x9;Or &#xA;&#x9;IsNull(b.catjes, 'NO DEFINIDO') Collate SQL_Latin1_General_CP1_CI_AS&lt;&gt; IsNull(a.catjes, 'NO DEFINIDO')&#xA;&#x9;)&#xA;&#xA;&#xA;;With dt As (Select Distinct a.CodPrv, a.ItemCode, CodFam, a.CodCat, b.NomCat, b.NomCat_Corto2, CatJes&#xA;&#x9;&#x9;&#x9;From SrvSAP.DW.Reportes.item_unidadvolumenpeso a&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; &#xA;&#x9;&#x9;&#x9;Left Join SrvSAP.Roma_Productiva.reportes.grupocategoria b On /*a.codprv = b.codprv And */ a.codcat=b.codcat)&#xA;Update a Set NomCat = Isnull(b.Nomcat, 'NO DEFINIDO'), NomCat_Abr = IsNull(b.nomcat_corto2, 'NO DEFINIDO')--, CatJes = IsNull(b.CatJes, 'NO DEFINIDO')&#x9;&#xA;From ventas.DimProductosFull a&#xA;Inner Join dt b On a.CodPrv = b.codprv Collate SQL_Latin1_General_CP1_CI_AS&#xA;And a.CodPro = b.itemCode Collate SQL_Latin1_General_CP1_CI_AS&#xA;Where (IsNull(a.NomCat, '*') &lt;&gt; IsNull(b.Nomcat, 'NO DEFINIDO') Collate SQL_Latin1_General_CP1_CI_AS&#xA;Or&#xA;IsNull(a.NomCat_Abr, '*') &lt;&gt; IsNull(b.nomcat_corto2, 'NO DEFINIDO') Collate SQL_Latin1_General_CP1_CI_AS&#xA;Or&#xA;IsNull(a.CatJes, '*') &lt;&gt; IsNull(b.CatJes, 'NO DEFINIDO') Collate SQL_Latin1_General_CP1_CI_AS&#xA;)&#xA;&#xA;&#xA;;With dt As (Select Distinct a.CodPrv, a.ItemCode, CodFam, a.CodCat, b.NomCat, b.NomCat_Corto2, CatJes&#xA;&#x9;&#x9;&#x9;From SrvSAP.DW.Reportes.item_unidadvolumenpeso a&#xA;&#x9;&#x9;&#x9;Left Join SrvSAP.Roma_Productiva.reportes.grupocategoria b On /*a.codprv = b.codprv And */ a.codcat=b.codcat&#x9;&#x9;&#x9;)&#xA;Update a Set CatJes = IsNull(b.CatJes, 'NO DEFINIDO')&#x9;&#xA;From ventas.DimProductosFull a&#xA;Inner Join dt b On a.CodPrv = b.codprv Collate SQL_Latin1_General_CP1_CI_AS&#xA;And a.CodPro = b.itemCode Collate SQL_Latin1_General_CP1_CI_AS&#xA;where IsNull(a.CatJes, '*') &lt;&gt; IsNull(b.CatJes, 'NO DEFINIDO') Collate SQL_Latin1_General_CP1_CI_AS&#xA;&#xA;UPDATE a Set CatJes = IsNull(b.catjes, 'NO DEFINIDO') Collate SQL_Latin1_General_CP1_CI_AS&#xA;From ventas.DimProductosFull a&#xA;Inner Join SrvSAP.DW.Reportes.item_unidadvolumenpeso b On a.CodPro = b.ItemCode Collate SQL_Latin1_General_CP1_CI_AS&#xA;And IsNull(a.CatJes,'NO DEFINIDO') &lt;&gt; IsNull(b.catjes, 'NO DEFINIDO') Collate SQL_Latin1_General_CP1_CI_AS&#xA;&#xA;Update a Set NomFam = b.Name&#x9;&#xA;From Ventas.DimProductosFull a&#xA;Inner Join SrvSAP.Roma_Productiva.dbo.[@BKS_FAMILIAS] b On a.CodFam = b.code Collate SQL_Latin1_General_CP1_CI_AS&#xA;Where IsNull(a.NomFam, '*') &lt;&gt; b.Name Collate SQL_Latin1_General_CP1_CI_AS&#xA;&#x9;&#x9;&#x9;&#xA;Update Ventas.DimProductosFull Set CodCat = '000' Where CodCat Is Null&#xA;Update a Set NomCat = 'NO DEFINIDO', NomCat_Abr = 'NO DEFINIDO' From Ventas.DimProductosFull a Where CodCat = '000'&#xA;&#x9;&#x9;&#x9;&#xA;;With dt As (Select Distinct a.IdPrv, a.CodCat, c.NomCat, c.nomcat_corto2 NomCat_Abr &#xA;&#x9;&#x9;&#x9;From Ventas.DimProductosFull a&#xA;&#x9;&#x9;&#x9;Inner Join Ventas.DimProveedores b On a.CodPrv = b.CodPrv&#xA;&#x9;&#x9;&#x9;Inner Join srvSAP.roma_productiva.reportes.grupocategoria c &#xA;&#x9;&#x9;&#x9;On a.CodPrv = b.CodPrv Collate SQL_Latin1_General_CP1_CI_AS And a.CodCat = c.codcat Collate SQL_Latin1_General_CP1_CI_AS)&#xA;Insert Into Ventas.Dim_Categorias_xProveedor (IdPrv, CodCat, NomCat, NomCat_Abrv)&#xA;Select IdPrv, CodCat, NomCat, NomCat_Abr From dt a&#xA;Where Not Exists (Select * From Ventas.Dim_Categorias_xProveedor b Where a.IdPrv = b.IdPrv And a.CodCat = IsNull(b.CodCat, '000'))&#xA;&#xA;&#xA;&#xA;;With dt As (Select Distinct a.IdPrv, CodCat, NomCat, NomCat_Abr&#xA;&#x9;&#x9;&#x9;From Ventas.DimProductosFull a&#xA;&#x9;&#x9;&#x9;Inner Join Ventas.DimProveedores b On a.CodPrv = b.CodPrv)&#xA;Update x Set NomCat = y.NomCat, NomCat_Abrv = y.NomCat_Abr--, CatJes = y.CatJes&#xA;From ventas.Dim_Categorias_xProveedor x&#xA;Inner Join dt y On x.IdPrv = y.IdPrv And x.CodCat = y.CodCat&#xA;&#xA;&#xA;&#xA;/*&#xA;Insert Into ventas.Dim_Categorias_xProveedor (IdPrv, CodCat, NomCat, NomCat_Abrv, CatJes)&#xA;Select Distinct IdPrv, CodCat, NomCat, NomCat_Abr, IsNull(CatJes, 'NO DEFINIDO') CatJes &#xA;From ventas.DimProductosFull a&#xA;Where Not Exists (Select * From ventas.Dim_Categorias_xProveedor b &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;Where a.IdPrv = b.IdPrv And a.CodCat = b.CodCat &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;And IsNull(a.CatJes, 'NO DEFINIDO') = IsNull(b.CatJes, 'NO DEFINIDO'))&#xA;&#xA;*/&#xA;&#xA;Update a Set IdCat_xPrv = b.IdCat_xPrv&#x9;&#xA;-- Select *&#xA;From ventas.DimProductosFull a&#xA;Inner Join Ventas.Dim_Categorias_xProveedor b On a.IdPrv = b.IdPrv And a.CodCat = b.CodCat --And a.CatJes = b.CatJes&#xA;Where IsNull(a.IdCat_xPrv, 0) != b.IdCat_xPrv&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
