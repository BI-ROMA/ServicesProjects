    <DTS:Executable
      DTS:refId="Package\Registra y Actualiza Item_UnidadVolumenPeso"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Tarea Ejecutar SQL"
      DTS:DTSID="{AE3079E4-A7E4-4ACD-96B6-4BB70CC38D45}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Registra y Actualiza Item_UnidadVolumenPeso"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2019; © 2019 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{EE8785FD-6D43-4F47-BAF3-145D7203E39B}"
          SQLTask:SqlStatementSource="--Drop Table #itemsBase&#xA;--Drop Table #Prod_ConInvetario&#xA;&#xA;;With &#xA;dtBase As (Select Distinct CodPrv, CodFam, CodCat&#xA;&#x9;&#x9;&#x9;From dw.Reportes.item_unidadvolumenpeso&#xA;&#x9;&#x9;&#x9;Where IsNull(CodCat, '000')&lt;&gt; '000' And IsNull(CodFam, '000') &lt;&gt; '000'),&#xA;dt As (Select CodPrv, CodFam, Count(Distinct CodCat) QCats&#xA;&#x9;&#x9;&#x9;From dtBase x&#xA;&#x9;&#x9;&#x9;Group By CodPrv, CodFam&#xA;&#x9;&#x9;&#x9;Having Count(Distinct CodCat)=1&#xA;&#x9;&#x9;&#x9;)&#xA;Select a.*, c.nomcat, nomcat_corto2, QCats&#xA;Into #itemsBase&#xA;From dtBase a&#xA;Inner Join dt b On a.CodPrv = b.CodPrv And a.CodFam = b.CodFam&#xA;Inner Join reportes.grupocategoria c On c.codprv = a.CodPrv And c.codcat = a.codcat&#xA;&#xA;Update a Set CodCat = b.Codcat&#xA;From dw.Reportes.item_unidadvolumenpeso a&#xA;Inner Join #itemsBase b On a.CodPrv = b.CodPrv And a.CodFam = b.CodFam&#xA;Where a.CodCat Is Null And b.QCats = 1&#xA;&#xA;;With dt As (Select Distinct codpro From temp.Tmp_Inventarios Where cantidad &gt; 0)&#xA;Select * &#xA;Into #Prod_ConInvetario&#xA;From dt a&#xA;Where Not Exists (Select * From dw.Reportes.item_unidadvolumenpeso b Where a.codpro = b.itemcode)&#xA;&#xA;&#xA;;With dt As (Select Distinct ItemCode &#xA;&#x9;&#x9;&#x9;From INV1 &#xA;&#x9;&#x9;&#x9;Where Quantity &gt; 0 And Convert(Char(6), DocDate, 112)=Convert(Char(6), Getdate(), 112)&#xA;&#x9;&#x9;&#x9;And ItemCode &lt;&gt; 'PERCEPCIONES')&#xA;Insert Into #Prod_ConInvetario&#xA;Select * From dt a&#xA;Where Not Exists (Select * From dw.Reportes.item_unidadvolumenpeso b Where a.ItemCode = b.itemcode)&#xA;&#xA;;With &#xA;dtOITM As (Select Distinct IsNull(codprv_homo, a.CardCode) CardCode, U_BKS_FAMILIA, ItemCode, ItemName, CreateDate&#xA;&#x9;&#x9;&#x9;From OITM a&#xA;&#x9;&#x9;&#x9;Left Join reportes.rel_prv_homologos b On a.CardCode = b.codprv&#xA;&#x9;&#x9;&#x9;Where frozenFor = 'N' &#xA;&#x9;&#x9;&#x9;And (a.ItmsGrpCod Not In (181,208) Or (ItemCode In (Select ItemCode From #Prod_ConInvetario)))&#xA;&#x9;&#x9;&#x9;And CardCode Is Not Null And U_BKS_FAMILIA Is Not Null&#xA;&#x9;&#x9;&#x9;-- And itemcode In ('MOL800569', 'MOL800874')&#xA;&#x9;&#x9;&#x9;),&#xA;dtprdfamcat As (Select CardCode, U_BKS_FAMILIA, ItemCode, ItemName, b.*, CreateDate &#xA;&#x9;&#x9;&#x9;From dtOITM a&#xA;&#x9;&#x9;&#x9;Left Outer Join #itemsBase b On a.CardCode = b.codprv And a.U_BKS_FAMILIA = b.codfam And QCats = 1)&#xA;Insert Into dw.reportes.item_unidadvolumenpeso (itemcode, CodPrv, CodFam, CodCat, ContenidoOriginal, FeRegistro)&#xA;Select a.itemcode, CardCode CodPrv, U_BKS_FAMILIA CodFam, CodCat, ItemName ContenidoOriginal, a.CreateDate&#xA;From dtprdfamcat a&#xA;--Left  Join (Select Distinct itemcode &#xA;--&#x9;&#x9;&#x9;&#x9;From oitw &#xA;--&#x9;&#x9;&#x9;&#x9;Where substring(whscode,5,2)='PR' And OnHand &gt; 0) alm On a.ItemCode = alm.ItemCode &#xA;Where Not Exists (Select * From dw.Reportes.item_unidadvolumenpeso b Where a.ItemCode = b.itemcode)&#xA;&#xA;Update a Set CodFam = b.U_BKS_FAMILIA, FeModifica = Getdate()&#xA;From dw.Reportes.item_unidadvolumenpeso a&#xA;Inner Join (Select Distinct ItemCode, U_BKS_FAMILIA, U_bks_categoria &#xA;&#x9;&#x9;&#x9;From OITM Where frozenFor = 'N') b On a.itemcode = b.ItemCode&#xA;Where a.CodPrv Not Like '%000027' And IsNull(a.CodFam, '*') &lt;&gt; b.U_BKS_FAMILIA&#xA;&#xA;Update a Set CodFam = Left(c.U_indice, 3)+'000', FeModifica = Getdate()&#xA;From dw.Reportes.item_unidadvolumenpeso a&#xA;Inner Join OITM b On a.itemcode = b.ItemCode And a.CodPrv = b.CardCode And Left(SWW, 1) = 'H'&#xA;Inner Join [@BKS_CATEGORIA] c On b.U_bks_categoria = c.Code&#xA;Where CodPrv Like '%00027' And CodFam != Left(c.U_indice, 3)+'000'&#xA;&#xA;--;With dt As (Select Distinct CodPrv, codfam, codcat &#xA;--&#x9;&#x9;&#x9;From reportes.relfamcatgrupo Where codprv Like '%00027' And numano+nummes = 202108)&#xA;--Update a Set CodCat = b.CodCat&#xA;--From dw.Reportes.item_unidadvolumenpeso a&#xA;--Inner Join dt b On a.CodPrv = b.codprv And a.CodFam = b.codfam&#xA;--Inner Join OITM c On a.itemcode = c.ItemCode And a.CodPrv = c.CardCode And Left(SWW, 1) = 'H'&#xA;--Where a.CodPrv Like '%00027'&#xA;&#xA;&#xA;&#xA;--- Fecha Factura y Entrada&#xA;&#xA;;With &#xA;dtItems As (Select itemcode From dw.Reportes.item_unidadvolumenpeso),&#xA;dt As (Select t1.ItemCode, T0.Docdate, T1.Quantity, ROW_NUMBER() Over(Partition By t1.ItemCode Order By t0.DocDate Desc) Cor&#xA;&#x9;&#x9;From OPCH T0&#xA;&#x9;&#x9;INNER JOIN PCH1 T1 ON T0.DocEntry = T1.DocEntry&#xA;&#x9;&#x9;Where Year(t0.DocDate) &gt;= 2018 And T1.ItemCode In (Select * From dtItems))&#xA;Update a Set FeFactura = b.DocDate&#xA;--Select * &#xA;From dw.Reportes.item_unidadvolumenpeso a&#xA;Inner Join dt b On a.itemcode = b.ItemCode&#xA;Where cor = 1&#xA;&#xA;&#xA;&#xA;;With &#xA;dtItems As (Select itemcode From dw.Reportes.item_unidadvolumenpeso),&#xA;dt As (Select t1.ItemCode, T0.Docdate, t1.ShipDate, T1.Quantity, ROW_NUMBER() Over(Partition By t1.ItemCode Order By t0.DocDate Desc) Cor&#xA;&#x9;&#x9;From OPDN T0&#xA;&#x9;&#x9;INNER JOIN PDN1 T1 ON T0.DocEntry = T1.DocEntry&#xA;&#x9;&#x9;Where Year(t0.DocDate) &gt;= 2018 And T1.ItemCode In (Select * From dtItems))&#xA;Update a Set FeEntrada = b.ShipDate&#xA;--Select * &#xA;From dw.Reportes.item_unidadvolumenpeso a&#xA;Inner Join dt b On a.itemcode = b.ItemCode&#xA;Where cor = 1&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
