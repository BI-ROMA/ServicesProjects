      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{D7F8252E-4B15-4E64-9890-03F0EC381A92}"
          SQLTask:SqlStatementSource="Update a Set fefactura=b.fefactura, feentrada=b.feentrada&#xA;-- Select *&#xA;From ROMA_REPOSITORIO.Nube.Item_UnidadVolumenPeso a&#xA;Inner Join SrvSAP.dw.Reportes.Item_UnidadVolumenPeso b On a.CodPrv = b.CodPrv Collate SQL_Latin1_General_CP1_CI_AS&#xA;And a.ItemCode = b.ItemCode Collate SQL_Latin1_General_CP1_CI_AS" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
