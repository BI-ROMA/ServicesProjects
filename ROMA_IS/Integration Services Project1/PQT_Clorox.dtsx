      <DTS:VariableValue
        DTS:DataType="8">select CodSede CodSedeAlmacen, 'ICA.01.170369' CodigoDistribuidor, a.ItemCode CodigoProducto , ItemName NombreProducto    , '' proveedor,'' grupo,'' Subgrupo1 ,'' Subgrupo2,'' Subgrupo3,''Subgrupo4,'' Subgrupo5, cast(a.NumInBuy as integer) FactorUnid,cast(a.NumInSale as integer) FactorVta ,case when a.ItmsGrpCod in (50,181,208) then '1' else  case when substring(a.ItemCode,4,1)='P' then '1' else '0' end end Bonificacion, 0.00 Peso,cast(b.costo*a.NumInBuy as numeric(14,4)) PrecioCompra,cast(p.Price*a.NumInSale as numeric(14,4)) PrecioSugerido,cast(a.avgprice*a.NumInSale as numeric(14,4)) PrecioPromedio from (select distinct ItemCode, CodSede from jyj.TmpVentas) v  inner join oitm a on v.ItemCode= a.ItemCode and a.TreeType='N'  inner join itm1 p on p.ItemCode =a.ItemCode and p.PriceList=12   left outer join reportes.ultpreciocompra b on a.ItemCode =b.itemcode and b.fecha =(select max(fecha) from reportes.ultpreciocompra x,oitm y where x.itemcode =y.ItemCode and y.CardCode ='PM00000021')</DTS:VariableValue>
