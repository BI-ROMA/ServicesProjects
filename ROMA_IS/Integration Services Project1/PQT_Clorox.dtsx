      <DTS:VariableValue
        DTS:DataType="8">select  		Substring(CodAlmacen, 3, 2) CodSedeAlm, 		'ICA.01.170369' CodigoDistribuidor,  		CodAlmacen CodigoAlmacen, 		NomAlmacen NombreAlmacen, 		e.CodPrv CodigoProveedor, 		e.NomPrv Proveedor, 		d.CodPro CodigoProducto, 		(a.Cantidad * d.QtdPiezas) StockEnUnidadDeConsumo, 		'PZA' UnidadDeMedidaDeConsumo, 		a.Cantidad StockEnUnidadDeCompra, 		d.CoUnidadMedidaCompra UnidadDeMedidaDeCompra, 		a.Valorizado ValorStock, 		c.Fecha FechaStock, 		0 IngresosEnUnidadDeConsumo, 		0.00 ValorIngresos, 		0 VentasEnUnidadDeConsumo, 		0.00 ValorVentas, 		0 OtrosEnUnidadDeConsumo, 		0 ValorOtros, 		month(c.Fecha) Periodo from [ROMA_DATAMART].[Ventas].[Fact_Inventarios_Acum] a inner join 	ventas.DimAlmacenes b on a.IdAlmacen = b.IdAlmacen inner join ventas.DimFechas c on a.FechaKey = c.FechaKey inner join ventas.DimProductosFull d on a.IdProducto = d.IdPro inner join ventas.DimProveedores e on a.IdProveedor = e.IdPrv where IdProveedor =1 and c.Fecha = dateadd(day,-1,'@@FechaFin@@') and a.IdAlmacen in (1,3,4)</DTS:VariableValue>
