        DTS:DataType="8">select  		Convert(Char(2), Substring(j.CodAlmacen, 3, 2)) CodSedeAlm, 		'ICA.01.170369' CodigoDistribuidor, 		h.docentry Docentry, 		convert(varchar(30), case h.TiDocumento when 'BV' then 'BO' when 'NC' then 'NC'  else 'FA' end +h.Serie+h.Numero) NroFactura, 		convert(varchar,b.Fecha) as Fecha, 		d.CodCli + +IsNull(d.CodDom, '001') CodigoCliente, 		a.Item LineNum, 		Convert(Varchar(20), c.CodPro) Itemcode, 		a.Cantidad Quantity, 		a.Total LineTotal, 		a.TotalIGV GTotal, 		a.PriceBefDi*(a.DiscPrcnt/100)*a.Cantidad*(1+a.VatPrcnt/100) descuento, 		a.u_bks_boni IndiCombo, 		''  CodCombo, 		Convert(Varchar(10), a.CodigoPromocion) CodigoPromocion, 		e.CodVen CodigoVendedor, 		0 CantidadDevuelta, 		case U_BKS_Boni when 'B' then a.priceBefDi * Cantidad*(1+a.VatPrcnt/100)  else 0 end BonificacionSoles, 		case U_BKS_Boni when 'B' then a.Cantidad  else 0 end BonificacionUnidades, 		Convert(Char(3), i.CodMesa) mesa, 		d.CodDom domi, 		month(b.Fecha) periodo  	From Planillas.vw_Fact_Ventas_Full a 	inner join Ventas.DimFechas b on a.FechaKey = b.FechaKey 	inner join ventas.DimProductosFull c on a.IdProducto =c.IdPro 	inner join ventas.DimClientes d on d.IdCli = a.IdCliente 	inner join ventas.DimVendedores e on e.IdVen = a.IdVendedor 	inner join ventas.DimSedes f on f.IdSede = a.IdSede 	inner join ventas.DimDocumentos h on h.IdDoc = a.IdDocumento 	inner join ventas.DimMesas i on i.IdMesa = a.IdMesa 	inner join ventas.DimAlmacenes j on j.IdAlmacen = f.IdAlmacen 	Where IdProveedor = 1 And b.fecha between '@@FechaIni@@' and '@@FechaFin@@';</DTS:VariableValue>
