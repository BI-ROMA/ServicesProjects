                <SQLTask:SqlTaskData
                  SQLTask:Connection="{D7F8252E-4B15-4E64-9890-03F0EC381A92}"
                  SQLTask:SqlStatementSource=";With &#xA;dt As (Select Distinct CodigoCliente, CodigoDistribuidor, CodigoVendedor, FeRegistro From [J&amp;J].Ventas a Where a.FeRegistro = ?),&#xA;dt1 As (Select CardCode+'001' CardCodeFull, * From SrvSAP.Roma_Productiva.dbo.OCRD Where Left(CardCode, 2) = 'EM'),&#xA;dt2 As (Select * From dt a Where Not Exists (Select * From [Clorox].extractor_clientes b Where a.CodigoCliente = b.CodigoCliente And b.FeRegistro = ? And a.CodigoDistribuidor = b.CodigoDistribuidor))&#xA;Insert Into [Clorox].extractor_clientes &#xA;Select CardCodeFull, b.cardName, '' RUC, LicTradNum, 'BODEGA', '', '', '', CodigoVendedor,'', 'A', '', '', 'MINORISTA', CodigoDistribuidor, FeRegistro&#xA;From dt2 a&#xA;Inner Join dt1 b On a.CodigoCliente = b.CardCodeFull Collate SQL_Latin1_General_CP1_CI_AS&#xA;&#xA;;With &#xA;dt As (Select FeRegistro, CodigoCliente, Count(*) Q From [Clorox].extractor_clientes  Where FeRegistro = ?&#xA;&#x9;&#x9;Group By FeRegistro, CodigoCliente Having Count(*)&gt;1),&#xA;dt1 As (Select FeRegistro, CodigoCliente, Zona, Distrito, DiaVisita From [Clorox].extractor_clientes &#xA;&#x9;&#x9;Where FeRegistro = ? And CodigoCliente In (Select Distinct CodigoCliente From dt) &#xA;&#x9;&#x9;And IsNull(Zona, '') &lt;&gt; '')&#xA;Update a Set Zona = b.Zona, Distrito = b.Distrito, DiaVisita = b.DiaVisita&#xA;From [Clorox].extractor_clientes a&#xA;left Join dt1 b On a.CodigoCliente = b.CodigoCliente&#xA;Where a.FeRegistro = b.FeRegistro And a.CodigoCliente In (Select Distinct CodigoCliente From dt) &#xA;And IsNull(a.Zona, '') = ''" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
