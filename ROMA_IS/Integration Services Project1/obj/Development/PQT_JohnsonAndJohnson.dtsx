            contenido = String.Empty;
            if (File.Exists(fichero))
            {
                contenido = File.ReadAllText(fichero);
                Dts.Variables["User::vPlantilla_Productos"].Value = contenido;
            }

            fichero = ruta + "\\Vendedores.txt";
            contenido = String.Empty;
            if (File.Exists(fichero))
            {
                contenido = File.ReadAllText(fichero);
                Dts.Variables["User::vPlantilla_Vendedores"].Value = contenido;
            }

            fichero = ruta + "\\Stock.txt";
            contenido = String.Empty;
            if (File.Exists(fichero))
            {
                contenido = File.ReadAllText(fichero);
                Dts.Variables["User::vPlantilla_Stock"].Value = contenido;
            }

            Dts.TaskResult = (int)ScriptResults.Success;
		}

        public static void ReadFile(string file)
        {
            StreamReader myReader = new StreamReader(file);
            string line = "";
            string sResult = "";
            while (line != null)
            {
                line = myReader.ReadLine();
                if (line != null)
                    //Console.WriteLine(line);
                    sResult = sResult + line;
            }
            myReader.Close();
            MessageBox.Show(sResult);
        }

