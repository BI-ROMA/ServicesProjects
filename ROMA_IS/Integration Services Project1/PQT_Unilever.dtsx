pe = 'S' AND ISNULL(c.U_BKS_CodDom, '001') = ISNULL(a.domi, '001') LEFT OUTER JOIN gProc.gcrd2 AS d ON d.cardcode = c.CardCode AND d.domi = c.U_BKS_CodDom LEFT OUTER JOIN uni.giros_unilever AS e ON e.cardcode = c.CardCode</DTS:VariableValue>
