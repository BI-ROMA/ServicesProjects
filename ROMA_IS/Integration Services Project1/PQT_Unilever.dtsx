      <DTS:VariableValue
        DTS:DataType="8">select '30281223' CodigoDistribuidor,  a.WhsCode     CodigoAlmacen   , c.WhsName  NombreAlmacen ,b.CardCode CodigoProveedor ,d.CardName Proveedor,b.ItemCode CodigoProducto, cast(a.OnHand as integer) StockEnUnidadDeConsumo,'PZA' UnidadDeMedidaDeConsumo,cast(a.OnHand/(b.NumInBuy*1.00) as numeric(14,4)) StockEnUnidadDeCompra,b.BuyUnitMsr UnidadDeMedidaDeCompra, cast(a.OnHand*e.Price as numeric(14,4))  ValorStock,convert(char(10),dateadd(day,-1,docdate),120)  FechaStock,cast(isnull(ig.cantidad,0) as integer)  IngresosEnUnidadDeConsumo, cast(isnull(ig.cantidad,0)*e.Price as numeric(14,2))  ValorIngresos,cast(isnull(vta.cantidad,0) as integer)  VentasEnUnidadDeConsumo, cast(isnull(vta.cantidad,0)*e.Price as numeric(14,2)) ValorVentas,0 OtrosEnUnidadDeConsumo,0 ValorOtros,month(dateadd(day,-1,docdate)) Periodo from dw.reportes.stockdiario a inner join OITM b on a.ItemCode=b.ItemCode and convert(char(8), a.docdate, 112)='@@FechaFin@@'   inner join OWHS c on c.WhsCode=a.WhsCode inner join ocrd d on d.CardCode=b.CardCode     inner join itm1 e on e.ItemCode=b.ItemCode and e.PriceList=12    left outer join (select y.WhsCode ,y.itemcode,sum(y.Quantity*y.NumPerMsr ) cantidad  				from opdn x inner join pdn1 y on x.objtype=y.objtype and x.docentry=y.docentry 				 and x.cardcode='PM00000059' and x.CANCELED='N' and x.docdate='@@FechaFin@@' group by y.itemcode,y.WhsCode) ig  on ig.ItemCode =a.ItemCode and ig.WhsCode=a.WhsCode   left outer join (select codalm whscode,codpro itemcode, 				sum(case tipo when 'V' then cantidad else -cantidad end ) cantidad  				from reportes.resvtaprvdia_ped  				where codprv='PM00000059' and fecvta='@@FechaFin@@' 				group by codalm,codpro) vta on vta.ItemCode =a.ItemCode and vta.whscode=a.WhsCode   where a.WhsCode in ('11ICPR1','11CHPR1','05AYPR1') and b.CardCode='PM00000059'</DTS:VariableValue>
