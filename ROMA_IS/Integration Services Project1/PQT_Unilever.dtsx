      DTS:ObjectName="vClientes">
      <DTS:VariableValue
        DTS:DataType="8">SELECT   '30281223' AS CodigoDistribuidor, b.CardCode + a.domi AS CodigoCliente, b.CardName AS name, CASE dbo.check_ruc(b.LicTradNum) WHEN 1 THEN b.LicTradNum ELSE '' END AS RUC, CASE dbo.check_ruc(b.LicTradNum) WHEN 1 THEN '' ELSE b.LicTradNum END AS DNI, LEFT(c.Street, 10) AS Street, ISNULL(b.Cellular, '') AS telefono,  b.E_Mail AS Email, a.canal, ISNULL(e.giro, 'MINORISTA-BODEGA') AS TipoNegocio, a.ruta AS Zona, ' ' AS Modulo, ISNULL(LEFT(c.U_BKP_UBIGEO, 6), '') AS Distrito, CAST(CAST(c.U_BKS_Refy AS numeric(14, 4)) AS varchar) + ',' + CAST(CAST(c.U_BKS_refx AS numeric(14, 4)) AS varchar) AS Georeferencia, a.slpcode, CASE len(replace(dia, '0', ''))  WHEN 1 THEN '0' + replace(dia, '0', '') + '-' + [dbo].[fn_get_nomdiaSemana](replace(dia, '0', '')) ELSE '09-TODOS LOS DIAS' END AS DiaVisita, 'A' AS Status, '' AS CodCorporativo, '' AS Mercado, '' AS Clasificacion FROM     (SELECT   cardcode, domi, MAX(canal) AS canal, ruta, MIN(slpcode) AS slpcode, MAX(dia) AS dia FROM      (SELECT   U_BKP_CLIENTE AS cardcode, U_BKP_CODDOM AS domi, RIGHT(u_bkp_codcnl, 2) AS canal, r.Name AS ruta, U_BKP_CODDIA AS dia, 0 AS slpcode     FROM      [@BKS_CLIENTE_MESA] AS cm INNER JOIN                     [@BKS_RUTAS] AS r ON cm.U_BKP_CODRUT = r.Code     WHERE    (U_BKP_CODMESA IN ('')) AND (U_BKP_ESTADO = 'A')     UNION     SELECT   U_BKP_CLIENTE AS cardcode, U_BKP_CODDOM, RIGHT(u_bkp_codcnl, 2) AS canal, r.Name AS ruta, U_BKP_CODDIA, t.slpcode     FROM     [@BKS_CLIENTE_MESA] AS cm INNER JOIN                 [@BKS_RUTAS] AS r ON cm.U_BKP_CODRUT = r.Code INNER JOIN                     (SELECT DISTINCT cardcode, domi_new2 AS domi, mesa_new2 AS mesa, slpcode                     FROM     uni.tmpVentasCab) AS t ON t.cardcode = cm.U_BKP_CLIENTE + cm.U_BKP_CODDOM AND cm.U_BKP_CODMESA = t.mesa AND t.domi = cm.U_BKP_CODDOM) AS w GROUP BY cardcode, domi, ruta) AS a INNER JOIN OCRD AS b ON a.cardcode = b.CardCode INNER JOIN crd1 AS c ON b.cardcode = c.cardcode AND c.AdresType = 'S' AND ISNULL(c.U_BKS_CodDom, '001') = ISNULL(a.domi, '001') LEFT OUTER JOIN gProc.gcrd2 AS d ON d.cardcode = c.CardCode AND d.domi = c.U_BKS_CodDom LEFT OUTER JOIN uni.giros_unilever AS e ON e.cardcode = c.CardCode</DTS:VariableValue>
