      <DTS:VariableValue
        DTS:DataType="8">select distinct a.DocEntry ,'30281223' CodigoDistribuidor,a.CardCode+isnull(a.U_BKS_DOMICILIO,'001') cardcode, a.SlpCode  ,a.docnum CDPedido, cast(Convert(Char(8), a.TaxDate, 112) as date) CDPedFecha, case isnull(anu.U_SerieNCFact,'A') when 'A' then  'APPRO' else 'CANC' end CDPedStatus,  case a.U_BKP_BOL_FACT when 'BV' then 'BO' else 'FA' end +isnull(substring(anu.U_SerieNCFact,1,charindex('-',anu.U_SerieNCFact)-1)+'-'+anu.U_NCFact,substring(a.U_BPV_SERI,1,charindex('-',a.U_BPV_SERI)-1)+'-' + a.U_BPV_NCON2 ) CDFactura,cast(Convert(Char(8), a.TaxDate, 112) as date) CDFactFecha,case isnull(anu.U_SerieNCFact,'A') when 'A' then  'LIBER' else 'CANC' end CDFactStatus,  case isnull(anu.U_SerieNCFact,'A') when 'A' then  ' ' else '07'  end   MotivoCancelado,a.U_BKP_CODMESA mesa,a.U_BKS_DOMICILIO domi, month(a.TaxDate) periodo   from OINV a inner join  INV1 b on  a.docentry=b.DocEntry  inner join oitm it on b.ItemCode=it.ItemCode and it.CardCode='PM00000059'   left outer join OSLP c on (a.SlpCode=c.SlpCode  ) left outer join (select U_SerieNCFact, U_NCFact,U_NoFactInt from  [@OK1_ANULTRANS] where createdate&gt;='@@FechaIni@@' and u_tipoTransac='V')  anu on anu.U_NoFactInt =a.DocEntry  where Convert(Char(8), Convert(Char(8), a.TaxDate, 112), 112)&gt;='@@FechaIni@@'  and Convert(Char(8), a.TaxDate, 112)&lt;='@@FechaFin@@'     union all  select distinct a.DocEntry ,'30281223' CodigoDistribuidor,a.CardCode+isnull(a.U_BKS_DOMICILIO,'001') cardcode, a.SlpCode  ,a.docnum CDPedido, cast(Convert(Char(8), a.TaxDate, 112) as date) CDPedFecha, case isnull(anu.U_SerieNCFact,'A') when 'A' then  'APPRO' else 'CANC' end CDPedStatus,  'NC' +isnull(substring(anu.U_SerieNCFact,1,charindex('-',anu.U_SerieNCFact)-1)+'-'+anu.U_NCFact,substring(a.U_BPV_SERI,1,charindex('-',a.U_BPV_SERI)-1)+'-' + a.U_BPV_NCON2 ) CDFactura, cast(Convert(Char(8), a.TaxDate, 112) as date) CDFactFecha,case isnull(anu.U_SerieNCFact,'A') when 'A' then  'LIBER' else 'CANC' end CDFactStatus, case isnull(anu.U_SerieNCFact,'A') when 'A' then ' '  else '07' end   MotivoCancelado,a.U_BKP_CODMESA mesa,a.U_BKS_DOMICILIO domi , month(Convert(Char(8), a.TaxDate, 112)) periodo  from ORIN  a inner join  rin1 b on  a.docentry=b.DocEntry  inner join oitm it on b.ItemCode=it.ItemCode and it.CardCode='PM00000059'    left outer join OSLP c on (a.SlpCode=c.SlpCode  ) left outer join (select U_SerieNCFact, U_NCFact,U_NoFactInt from  [@OK1_ANULTRANS] where createdate&gt;='@@FechaIni@@' and u_tipoTransac='V')  anu on anu.U_NoFactInt =a.DocEntry  where Convert(Char(8), a.TaxDate, 112)&gt;='@@FechaIni@@'  and Convert(Char(8), a.TaxDate, 112)&lt;='@@FechaFin@@'         and not exists (select 1 from uni.anulados  an  where an.objtype = a.objtype and an.docentry = a.docentry )</DTS:VariableValue>
